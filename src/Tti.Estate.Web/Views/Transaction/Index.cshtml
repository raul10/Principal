@model TransactionListModel
@{
    ViewData["Title"] = Localizer["Title"];
}
<partial name="_CriteriaButtonPartial" for="Criteria" />
<a asp-action="Create" class="btn btn-primary">
    @Localizer["Create"]
</a>
<div class="table-responsive">
    <table class="table table-hover mt-3">
        <thead>
            <tr>
                <th>@Localizer["TransactionType"]</th>
                
                 
                <th>@Localizer["Date"]</th>
 
            </tr>
        </thead>
        <tbody>
            @Html.DisplayFor(x => x.Transactions.Items)
        </tbody>
   
    </table>
</div>
<partial name="_PaginationPartial" model="Model.Transactions" />
<partial name="_EmptyResultPartial" model="Model.Transactions" />
<partial name="_CriteriaModalPartial" />